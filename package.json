{
    "name": "warpcode-vim-config",
    "engines": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": [
                    "all"
                ],
                "path": "./snippets/all/file.json"
            },
            {
                "language": [
                    "martini-html"
                ],
                "path": "./snippets/martini-html.json"
            },
            {
                "language": [
                    "martini-php"
                ],
                "path": "./snippets/martini-php.json"
            },
            {
                "language": [
                    "php"
                ],
                "path": "./snippets/php.json"
            },
            {
                "language": [
                    "shellscript",
                    "shell",
                    "sh",
                    "zsh"
                ],
                "path": "./snippets/shell.json"
            }
        ]
    },
    "dependencies": {
        "neovim": "latest",
        "yarn": "latest"
    }
}
